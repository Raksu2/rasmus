let parool = Math.floor(Math.random() * 10 + 1
console.log(parool)
password = prompt("sisesta üks number 1-10-ni")
if (password == parool)
console.log("YAY! sa pakkusid õige numbri!")
else if (password - 1 ==parool // password + 1 == parool)
console.log("oled lähedal")
else
